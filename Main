package Lesson_1.Marathon;


public class Main {
    public static void main(String[] args) {
        Team team = new Team("Боб и его команда", new Human("Серёжа"), new Cat("Снежок"), new Human("Боб"), new Cat("Барсик"), new Dog("Бобик"));
        Course c = new Course(new Cross(80), new Water(5), new Wall(1), new Cross(120));
        c.doIt(team.competitors); // Просим команду пройти полосу
        team.showResults(); // Показываем результаты
    }
}